<local:WindowExt x:Class="DatatehTestTask.WindowOne"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatatehTestTask"
        mc:Ignorable="d"
        Title="WindowOne" Height="140" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Grid.Column="0" Style="{StaticResource BaseStyle}" Command="{Binding Path=(local:WindowOneViewModel.FirstButtonCommand)}">Кнопка 1</Button>
        <Button Grid.Column="1" Style="{StaticResource BaseStyle}" Command="{Binding Path=(local:WindowOneViewModel.SecondButtonCommand)}">Кнопка 2</Button>
    </Grid>
</local:WindowExt>
